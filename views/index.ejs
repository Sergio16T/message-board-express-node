<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <table>
      <thead>
        <tr>
          <th> User </th>
          <th> Text </th>
          <th> Date Added </th>
        </tr>
      </thead>
      <tbody> 
        <% messages.forEach(message => { %>
        <tr>
          <td><%= message.user %></td>
          <td><%= message.text %></td>
          <td><%= message.added %></td>
        </tr>
       <% }) %>
       
      <tbody> 
    </table>
    <a href="new"><button>Add Message!</button></a>
  </body>
</html>
